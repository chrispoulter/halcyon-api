{
    "root": true,
    "extends": "react-app",
    "settings": {
        "react": {
            "version": "999.999.999"
        }
    },
    "rules": {
        "no-template-curly-in-string": "off",
        "react-hooks/exhaustive-deps": "off",
        "no-unused-vars": [
            "error",
            {
                "vars": "all",
                "varsIgnorePattern": "^_",
                "args": "after-used",
                "argsIgnorePattern": "^_"
            }
        ]
    }
}
